  const reveal = () => wrap.classList.add('revealed');\n\n  if ('IntersectionObserver' in window){\n    const io = new IntersectionObserver((e,o)=>{\n      if (e[0].isIntersecting){ reveal(); o.disconnect(); }\n    },{threshold:.4});\n    io.observe(wrap);\n  } else {\n    /* simple fallback */\n    const onScroll = () => {\n      const rect = wrap.getBoundingClientRect();\n      if (rect.top < window.innerHeight*.6){ reveal(); window.removeEventListener('scroll',onScroll); }\n    };\n    window.addEventListener('scroll',onScroll,{passive:true});\n    onScroll();                                  /* in case already in view */\n  }\n})();\n</script>"
